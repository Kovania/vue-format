{
    "name": "vue-format",
    "displayName": "vue-format",
    "description": "",
    "version": "0.0.1",
    "publisher": "vue-format",
    "engines": {
        "vscode": "^1.22.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.vueFormat"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [{
            "when": "resourceLangId == vue",
            "command": "extension.vueFormat",
            "title": "Vue Format"
        }],
        "menus": {
            "editor/context": [{
                "when": "resourceLangId == vue",
                "command": "extension.vueFormat",
                "group": "navigation"
            }]
        }
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "js-beautify": "^1.7.5"
    }
}
